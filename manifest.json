{
    "manifest_version": 2,
 
    "name": "Paranoya UI design",
    "version": "1.0",
    "description":"Demo plugin",
    "content_scripts": [
        {
            "matches": [ "http://pqpqpq.ru/admin/*"],
            "js":["jquery.js"]
        }
    ],
    "icons": {
        "128": "icon.png",
        "16": "icon_16.png"
    },
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "off.png",
        "default_title": "Make it beautiful"
    },
    "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ]
}
